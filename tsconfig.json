{
    "compilerOptions": {
        "target": "es6",
        "module": "CommonJS",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "paths": {
            "@routes":["./src/routes/index.ts"],
            "@utils":["./src/utils/index.ts"],
            "@DB":["./DB/index.ts"]
        }
    },
    "include": ["src/**/*.ts", "DB/**/*.ts", "src/**/*.html"],
    "exclude": ["node_modules"]
}